#include<stdio.h>
int main()
{
    int a=3,b=6;
    printf("%d\n",a+b);
    printf("%d\n",a>b);
     printf("%d\n",a>=b); 
     printf("%d\n",a!=b);
     printf("%d\n",(a>b) &&(a<b)); 
     printf("%d\n",a||b); 
     printf("%d\n",a&b); 
     printf("%d\n",a|b); 
     printf("%d\n",a>>1);
     printf("%d\n",a<<2);
      printf("%d\n",~a);
}
    
