#include<stdio.h>
int main()
{
	int n,i=0,d=0,r;
	printf("enter the number of counts:");
	scanf("%d",&n);
	while(i<=n)
	{
		scanf("%d",&r);
		if(r>0)
		{
		d=d+r;
		}
		i++;
	}
	printf("sum is %d",d);
}

#include<stdio.h>
int main()
{
	int n,i=1,d=0,a;
	printf("enter the number of count:");
	scanf("%d",&n);
	while(i<=n)
	 {
		 scanf("%d",&a);
		 if(a>0)
		 {
			 d=d+a;
			 i++;
		 }
		 else
			 break;

	 }
	 
	 printf("sum of positive numbers is  = %d",d);
}

