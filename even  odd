#include <stdio.h>
#include <stdlib.h>
int main()
{
    int i,num;
    printf("Print all even number until\n");
    scanf("%d",&num);
    printf("Even number from 1 to %d are\n",num);
    for(i=1; i<=num; i++)
    {
        if(i%2==0)
        {
            printf("%d\n",i);
        }
    }
    printf(" odd numbers are \n");
    for(i=1; i<=num; i++)
    {//loop for iterates from 1 to maximum
        if(i%2==1)
        {
            printf("%d\n",i);
        }
    }
}
